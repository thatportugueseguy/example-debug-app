    "dev": "concurrently --kill-others-on-fail \"yarn start\" \"yarn start:server:dev\"",
    "prod": "yarn build && NODE_ENV=production PORT=3000 yarn start:server"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  },
  "proxy": "http://localhost:3001/",
