{
  "name": "debug-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "node-fetch": "^2.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "node server/index.js",
